GET /rsc -c 'ac:application/json' -s 'cc:max-age=10,no-cache;et:123' -e 'ch:false;st:200'
GET /rsc -c 'ac:application/json' -s 'cc:max-age=10,no-cache;et:123' -e 'ch:true;st:200' -e 'ch:false;st:200'

